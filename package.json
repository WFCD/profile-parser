{
  "name": "@wfcd/profile-parser",
  "version": "0.0.0",
  "description": "Parser for Digital Extreme's profile data",
  "repository": "https://github.com/WFCD/profile-parser",
  "type": "module",
  "exports": {
    "./Parser": {
      "types": "./dist/ProfileParser.d.ts",
      "default": "./dist/ProfileParser.js"
    },
    "./Profile": {
      "types": "./dist/Profile.d.ts",
      "default": "./dist/Profile.js"
    },
    "./XpInfo": {
      "types": "./dist/XpInfo.d.ts",
      "default": "./dist/XpInfo.js"
    },
    "./Stats": {
      "types": "./dist/Stats.d.ts",
      "default": "./dist/Stats.js"
    }
  },
  "public": true,
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "types"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@wfcd/eslint-config": "^1.6.3",
    "c8": "^10.1.3",
    "chai": "^6.0.1",
    "chai-json-equal": "^0.0.1",
    "husky": "^9.1.7",
    "install-peerdeps": "^3.0.7",
    "lint-staged": "^16.1.2",
    "mocha": "^11.7.1",
    "tsdown": "^0.15.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "tsdown",
    "build:docs": "tsdown && jsdoc -c .github/docs/conf.json -d docs",
    "coverage": "npm test && c8 report --reporter=text-lcov | npx -y -p coveralls coveralls",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "prepublishOnly": "npm run build",
    "test": "c8 mocha"
  },
  "author": "SlayerOrnstein",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "@wfcd/items": "^1.1270.626",
    "warframe-worldstate-data": "^3.0.0"
  },
  "publishConfig": {
    "provenance": true
  }
}
