{
  "extends": "@wfcd/eslint-config/esm",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "modules": true
    },
    "babelOptions": {
      "plugins": [
        "@babel/plugin-transform-class-properties",
        "@babel/plugin-transform-private-methods"
      ]
    }
  }
}